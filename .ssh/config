Include /Users/bernd_prager/.colima/ssh_config

Host odin
	HostName odin

Host 192.168.1.3
  HostName 192.168.1.3

Host bitbucket.org
  AddKeysToAgent yes
  IdentityFile ~/.ssh/bitbucket_work

Host *
	User bernd
	ForwardX11 yes
  ForwardX11Trusted yes
  ServerAliveInterval 120
  ServerAliveCountMax 300

Host qnap
  ForwardX11 no

Host github.com
  ForwardX11 no

# Jump host configuration
Host jump
    HostName prager.homeip.net
    User bernd  # username on the jump host
    Port 22     # optional if it's not the default

# Remote host configurations through jump host
Host jump-odin
    HostName odin
    User bernd
    ProxyJump jump  # refer to the 'jump' host config above
    Port 22

# Remote host configuration through jump host
Host jump-fenrir
    HostName fenrir
    User bernd
    ProxyJump jump  # refer to the 'jump' host config above
    Port 22

URxvt*font:     terminus-12
URxvt*boldFont: terminus-12

!Standard big font + black background
*.font: xft:monospace:style=Bold:size=15


#define FONTSIZE 18
!xterm*faceName: xft:PragmataPro:size=FONTSIZE:antialias=true:hinting=true:autohint=false:hintstyle=hintnone:dpi=96
!URxvt*font:     xft:PragmataPro:size=FONTSIZE:antialias=true:hinting=true:autohint=false:hintstyle=hintnone:dpi=96
!URxvt*boldFont: xft:PragmataPro:size=FONTSIZE:antialias=true:hinting=true:autohint=false:hintstyle=hintnone:dpi=96
!xterm*faceName:DejaVu Sans Mono:size=10:antialias=true

! Extra options for xft font spec - ~/.fonts.conf seems to override this
!:antialias=false:hintstyle=hintfull

#define WINDOWGEOMETRY 120x40
!xterm
xterm*dynamiccolors:      true
xterm*utf8:               2
xterm*geometry:           WINDOWGEOMETRY
xterm*visualBell:         off
xterm*highlightSelection: true
!xterm*background:      lightblack
!xterm*foreground:      lightblue
xterm*colorMode:       on
xterm*colorBD:         lightcyan
xterm*colorBDMode:     on
xterm*colorUL:         white
xterm*colorULMode:     on
xterm*scrollTtyOutput: false
xterm*scrollKey:       true
xterm*scrollBar:       true
xterm*termName:xterm-256color
xterm*utf8Title:true
xterm*xftAntialias:true
xterm*faceName:DejaVu Sans Mono:size=FONTSIZE:antialias=true
!xterm*faceName:Hack:size=FONTSIZE:antialias=true
!xterm*faceName:Source Code Pro:size=FONTSIZE:antialias=true
!xterm*faceName:Misc Fixed:size=FONTSIZE:antialias=true
Xft.dpi:96
xpdf.title:PDF
Xft.antialias:true
Xft.hinting:true
Xft.hintstyle:hintslight
xterm*faceNameDoublesize:Microsoft YaHei
xterm*faceSize:FONTSIZE
xterm*inputMethod:fcitx
xterm*background:black
xterm*foreground:grey
xterm*SaveLines:1000
xterm*VT100.translations: #override <Btn1Up>: select-end(PRIMARY, CLIPBOARD, CUT_BUFFER0)
Xcursor.theme:Breeze_Snow
Xcursor.size:24

! URxvt
URxvt.modifier: mod1
URxvt*loginShell: true
URxvt.borderless: false
URxvt*dynamicColors: on
URxvt*letterSpace: -1
URxvt.geometry: WINDOWGEOMETRY
URxvt.imLocale: en_US.UTF-8
URxvt.lineSpace: 0
URxvt.loginShell: true
URxvt.saveLines: 5000
URxvt.scrollBar: false
URxvt.scrollstyle: plain
URxvt*termName: xterm-256color
URxvt.transparent: false
URxvt.visualBell: false

*.boldMode: true
*.colorBDMode: false
